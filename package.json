{
  "name": "opentok-text-chat",
  "version": "3.0.0",
  "description": "OpenTok text chat accelerator pack",
  "main": "dist/opentok-text-chat.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:logversion": "replace '(js-vsol-)(x.y.z)' '$1'$npm_package_version dist/*",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:format": "prettier --config .prettierrc '**/*.ts' --write",
    "test": "mocha -r ts-node/register test/**/*spec.ts --insect",
    "coverage": "nyc mocha -r ts-node/register test/**/*spec.ts --insect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opentok/accelerator-textchat-js"
  },
  "keywords": [
    "opentok",
    "chat",
    "text",
    "webrtc"
  ],
  "author": "michael.jolley@vonage.com",
  "license": "MIT",
  "devDependencies": {
    "@opentok/client": "^2.18.2",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.14.6",
    "@types/sinon": "^9.0.8",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "chai": "^4.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "opentok-accelerator-core": "opentok/accelerator-core-js#typescript",
    "prettier": "^2.1.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "kuende-livestamp": "^1.1.2",
    "opentok-solutions-logging": "^1.1.0"
  }
}